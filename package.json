{
  "name": "riscript",
  "version": "1.0.21",
  "author": "Daniel C Howe",
  "description": "generative language scripting",
  "homepage": "https://github.com/dhowe/riscript",
  "license": "GPL-3.0",
  "type": "module",
  "scripts": {
    "test": "NODE_ENV=dev mocha",
    "test:dist": "NODE_ENV=dev mocha test/dist",
    "build": "rm -rf ./dist/ && npx tsup",
    "pub": "np patch",
    "types": "npx tsc"
  },
  "main": "./dist/riscript.js",
  "exports": {
    ".": {
      "import": "./dist/riscript.js",
      "require": "./dist/riscript.cjs",
      "types": "./dist/riscript.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "unpkg": "./dist/riscript.iife.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dhowe/riscript.git"
  },
  "engines": {
    "node": ">=14.0"
  },
  "keywords": [
    "natural language generation",
    "generative text"
  ],
  "bugs": {
    "url": "https://github.com/dhowe/riscript/issues"
  },
  "devDependencies": {
    "@types/chai": "^4.3.9",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.8.9",
    "chai": "^4.3.10",
    "esbuild-plugin-version-injector": "^1.2.1",
    "mocha": "^10.2.0",
    "np": "^8.0.4",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "chevrotain": "^11.0.3",
    "he": "^1.2.0",
    "mingo": "^6.4.8"
  },
  "peerDependencies": {
    "rita": "^3.0"
  },
  "peerDependenciesMeta": {
    "rita": {
      "optional": true
    }
  }
}
